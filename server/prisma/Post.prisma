model Post {
  id String @id @default(uuid())
  title String
  body String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  deletedAt DateTime?
  user User @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId String


  @@map("post")
}

model User{
  id String @id
  post Post[]
}

